{
  "common": {
    "disabled": ["create", "update", "get"]
  },
  "view": {
    "list": {
      "fields": ["description", "type", "size", "status", "createdAt", "updatedAt"],
      "stat": {
        "aggregate": [
          { "fields": ["type"], "group": "type", "aggregate": ["count"] },
          { "fields": ["status"], "group": "status", "aggregate": ["count"] }
        ]
      }
    }
  }
}
