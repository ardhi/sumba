{
  "multiSite": false,
  "waibu": {
    "title": "Sumba",
    "prefix": "site"
  },
  "waibuMpa": {
    "home": "sumba:/my-stuff/profile",
    "icon": "globe",
    "redirect": {
      "/": "sumba:/my-stuff/profile",
      "/my-stuff": "sumba:/my-stuff/profile",
      "/info": "sumba:/info/about-us",
      "/user": "sumba:/my-stuff/profile",
      "/db/export": "sumba:/db/export/list"
    }
  },
  "waibuAdmin": {
    "modelDisabled": "all"
  },
  "auth": {
    "common": {
      "apiKey": {
        "type": "Bearer",
        "qsKey": "apiKey",
        "headerKey": "X-Sumba-ApiKey"
      },
      "basic": {
      },
      "jwt": {
        "type": "Bearer",
        "qsKey": "token",
        "headerKey": "X-Sumba-Token",
        "secret": "668de9cf57316c7dbf52f7ff7611c299",
        "expiresIn": 604800000
      }
    },
    "waibuRestApi": {
      "methods": ["basic", "apiKey", "jwt"]
    },
    "waibuMpa": {
      "methods": ["session"]
    },
    "waibuStatic": {
      "methods": ["basic", "apiKey", "jwt"],
      "basic": {
        "useUtf8": true,
        "realm": "Protected Area",
        "warningMessage": "Please authenticate yourself, thank you!"
      }
    }
  },
  "redirect": {
    "signin": "sumba:/signin",
    "signout": "sumba:/signout",
    "home": "sumba:/"
  },
  "userPassword": {
    "minUppercase": 1,
    "minLowercase": 1,
    "minSpecialChar": 1,
    "minNumeric": 1,
    "noWhitespace": false,
    "latinOnlyChars": false
  }
}